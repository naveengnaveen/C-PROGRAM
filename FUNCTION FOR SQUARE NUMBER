#include<stdio.h>
#include<math.h>
int calculate_power(int a,int b);
int main()
{
    int a,b,power;
    printf("enter the base number:",a);
    scanf("%d",&a);
    printf("enter the exponent number:",b);
    scanf("%d",&b);
    power=calculate_power(a,b);
    printf("the answer: %d ",power);
    return 0;
}
int calculate_power(int a,int b)
{
    return pow(a,b);
}

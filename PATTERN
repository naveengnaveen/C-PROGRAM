#include<stdio.h>
void main()
{
    int a,i,j;
    printf("enter a size of the number:");
    scanf("%d \n",&a);
    for(i=0;i<a;i++)
    {
        for(j=0;j<=i;j++)
        {
            printf("*");
        }
        printf("\n");
    }
    int b,c,d;
    {
        printf("enter the size of b is");
        scanf("%d \n",&b);
        for(c=0;c<b;c++);
        {
            for(d=0;d<=b;d++);
            {
                printf("");
            }
            printf("\n");
        }
    }
    int w,r,t;
    {
        printf("enter the size of the w:");
        scanf("%d \n",&w);
        for(r=w;r>=0;r--);
        {
            for(t=0;t<=r;j++);
            {
                printf("8");
            }
            printf("\n");
        }
    }
}
